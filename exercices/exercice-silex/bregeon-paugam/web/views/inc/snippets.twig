{% for snippet in snippets %}

<div class="row">
  <div class="col-sm-6 col-md-6">
    <div class="thumbnail">
		<div class="caption">
			<h3>{{snippet.title}}</h3>
				<p><div>{{snippet.content}}</div></p>
			<div>
				<a class="btn btn-primary" role="button" href="{{url('category',{category:snippet.category_slug})}}">{{snippet.category_title}}</a>
			</div>
		</div>
  	</div>
  </div>
</div>			

{% endfor %}


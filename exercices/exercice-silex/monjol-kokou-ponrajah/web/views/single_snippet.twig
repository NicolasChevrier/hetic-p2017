{{include('inc/header.twig', {title:'Snippet'})}}

<!-- One Snippet Show -->
{% for snippet in snippets %}
			<!-- Title -->
			<h3>{{snippet.title}}</h3>
			<!-- Category -->
			<div>
				<a href="{{url('category', {category:snippet.category_slug})}}">{{snippet.category_title}}</a>
				<!-- Content -->
				<p>{{snippet.content}}</p>
			</div>
	{% endfor %}

{{include('inc/footer.twig')}}
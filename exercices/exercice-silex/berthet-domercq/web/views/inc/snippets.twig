{% for snippets in snippets %}
	<div class="snippets">
		<p class="snippetstitle">{{snippets.title}}</p>
		<div>
			<a class="snippetcategory" href="{{url('category',{category:snippets.category_slug})}}">{{snippets.category_title}}</a>
		</div>
		<div class="snippetcontent"><img src="{{snippets.content}}" alt="{{snippets.title}}" class="img_snippet"></div>
	</div>
{% endfor %}
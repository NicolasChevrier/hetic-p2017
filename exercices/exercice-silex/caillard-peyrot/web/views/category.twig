{% extends layout %}

{% block content %}
  <section class="col-md-12">
    <h2>Snippets in
      {% for c in categories %}
        {% if (c.slug == category) %}
          {{c.title}}
        {% endif %}
      {% endfor %}
    </h2>
    <hr>
    {{include('incs/snippets.twig')}}
  </section>
{% endblock %}
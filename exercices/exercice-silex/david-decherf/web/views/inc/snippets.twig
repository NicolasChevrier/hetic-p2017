{% for snippet in snippets %}
	<div>

		<ul class="list-group">
			<li class="list-group-item">
				<h3>{{snippet.title}}</h3>
		
		<a class="label label-default" href="{{url('category',{category:snippet.category_slug})}}">{{snippet.category_title}}</a>
		
		<div>{{snippet.content}}</div>
			</li>
		</ul>

	</div>
{% endfor %}